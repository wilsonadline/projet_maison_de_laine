{% extends "base.html.twig" %}

{% block title %}Page non trouvée{% endblock %}

{% block body %}
<div class="d-flex  flex-column justify-content-center align-items-center">
    <h1 class="mt-5">Page non trouvée</h1>
    <p class="mt-3" >La page que vous cherchez n'a pas été trouvée.</p>  
    <p class="mt-3">Code de l'erreur : {{status_code}}</p>  
    <p class="mt-3">Message de l'erreur : {{status_text}}</p>  

</div>
{% endblock %}